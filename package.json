{
  "name": "nitro-web",
  "description": "",
  "version": "0.0.1",
  "private": true,
  "main": "client.js",
  "type": "module",
  "imports": {
    "#nitro-web/*": { "default": "./*" }
  },
  "babelMacros": {
    "twin": {
      "preset": "emotion",
      "config": "./_example/tailwind.config.js"
    }
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --target=web",
    "dev": "clear  && npm run dev:server:lint --silent & npm run dev:server --silent & npm run dev:client --silent",
    "dev:client": "NITRO=true webpack serve --progress --config ./_example/webpack.config.js",
    "dev:server": "NITRO=true nodemon ./_example/server -q -w ./_example/server/ -w ./_example/components/ -w ./server/ -w ./components/ -e js",
    "dev:server:lint": "eslint ./_example/components ./_example/server ./components/ ./server/",
    "minor": "standard-version --release-as minor && git push staging master",
    "patch": "standard-version --release-as patch && git push staging master",
    "start": "node ./_example/server",
    "stripe": "stripe listen --forward-to localhost:3001/api/stripe/webhook"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@hokify/axios": "^0.19.1",
    "@stripe/react-stripe-js": "^1.9.0",
    "@stripe/stripe-js": "^1.34.0",
    "@uiw/color-convert": "^2.3.0",
    "@uiw/react-color-hue": "^2.3.0",
    "@uiw/react-color-saturation": "^2.3.0",
    "axios-retry": "^3.3.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "chart.js": "^4.4.3",
    "compression": "^1.7.4",
    "connect-mongo": "^5.1.0",
    "date-fns": "^3.6.0",
    "dateformat": "^3.0.3",
    "dotenv": "^14.3.2",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.6",
    "express-session": "^1.17.0",
    "inline-css": "^4.0.2",
    "monastery": "~3.5.1",
    "nanoid": "^4.0.0",
    "nodemailer": "^6.5.0",
    "nodemailer-mailgun-transport": "^2.0.2",
    "nunjucks": "^3.2.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pdf-to-img": "^4.1.0",
    "pdfmake": "0.2.7",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-currency-input-field": "^3.8.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-number-format": "^5.4.0",
    "react-router-dom": "6.24.1",
    "react-select": "^5.9.0",
    "react-tracked": "^1.3.0",
    "sort-route-addresses-nodeps": "0.0.4",
    "stripe": "^9.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.8.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.24.7",
    "@emotion/babel-plugin": "^11.11.0",
    "@emotion/eslint-plugin": "^11.11.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@svgr/webpack": "^8.1.0",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "autoprefixer": "^9.8.8",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-react-html-attrs": "^2.1.0",
    "clean-terminal-webpack-plugin": "https://github.com/boycce/clean-terminal-webpack-plugin.git",
    "color": "^4.2.3",
    "copy-webpack-plugin": "^12.0.2",
    "core-js": "^3.12.1",
    "css-loader": "^3.6.0",
    "csv-loader": "^3.0.5",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "eslint-webpack-plugin": "^2.7.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "migrate-mongo": "^10.0.0",
    "mini-css-extract-plugin": "^2.9.0",
    "nodemon": "^2.0.1",
    "postcss": "^8.4.49",
    "postcss-for": "^2.1.1",
    "postcss-import": "^16.1.0",
    "postcss-import-resolver": "^2.0.0",
    "postcss-loader": "^8.1.1",
    "postcss-nested": "^7.0.2",
    "react-refresh": "^0.14.2",
    "standard-version": "^9.3.2",
    "string-replace-loader": "^3.1.0",
    "super-nunjucks-loader": "^2.0.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.17",
    "twin.macro": "^3.4.1",
    "typescript": "^5.5.2",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.6.0",
    "webpack-node-externals": "^1.7.2"
  },
  "engines": {
    "node": "^18"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "nodemonConfig": {
    "events": {
      "restart": "echo \"\\033[0;35mExpress restarting..\\033[0m\""
    }
  },
  "standard-version": {
    "releaseCommitMessageFormat": "{{currentTag}}",
    "sign": true,
    "skip": {
      "changelog": true,
      "tag": true
    }
  },
  "prettier": {
    "printWidth": 140,
    "quoteProps": "consistent",
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "jest": {
    "transform": {}
  },
  "author": "",
  "license": "ISC"
}
